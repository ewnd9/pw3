{
  "name": "pw3",
  "version": "0.4.1",
  "description": "Air dates, search on tpb, eztv or kickass, check watched episodes from cli",
  "scripts": {
    "start": "node cli.js",
    "test": "mocha --require babel/register",
    "coverage": "istanbul cover _mocha src/index.js -- --require babel/register",
    "precoveralls": "npm run coverage",
    "coveralls": "coveralls < coverage/lcov.info"
  },
  "main": "lib/index.js",
  "bin": "cli.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewnd9/pw3.git"
  },
  "keywords": [
    "torrent",
    "imdb",
    "movies",
    "tv",
    "shows",
    "cli",
    "thepiratebay",
    "piratebay",
    "kickass",
    "subtitles"
  ],
  "author": "ewnd9 <ewndnine@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.5",
    "bluebird": "^2.9.30",
    "chalk": "^1.0.0",
    "cli-table": "^0.3.1",
    "dot-file-config": "0.4.0",
    "download": "^4.1.4",
    "eztv": "0.0.10",
    "inquirer-menu": "^0.1.2",
    "inquirer-question": "^1.1.1",
    "kexec": "^1.2.0",
    "lodash": "^3.2.0",
    "meow": "^3.3.0",
    "moment": "^2.10.3",
    "node-kickass-json": "^0.1.1",
    "opensrt_js": "git://github.com/ewnd9/OpenSRTJS.git#0cf6631f07725877ca13a3022b5ecf7d20188708",
    "pretty-bytes": "^2.0.1",
    "prompt": "^0.2.14",
    "request-promise": "^0.4.2",
    "spawn-args": "^0.1.0",
    "thepiratebay": "^0.2.2"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "coveralls": "^2.11.3",
    "istanbul": "^0.3.17",
    "mocha": "^2.2.5"
  },
  "app-data": {
    "programs": [
      {
        "name": "transmission-gtk",
        "script": "transmission-gtk \"$arg\"",
        "description": "Default ubuntu app",
        "type": "exec"
      },
      {
        "name": "peerflix",
        "script": "peerflix \"$arg\" --vlc",
        "description": "Torrent-streaming https://github.com/mafintosh/peerflix",
        "type": "replace"
      }
    ]
  }
}
